{
  "name": "mystorage",
  "version": "3.5.0",
  "description": "MyStorage application",
  "workspaces": [
    "apps/*"
  ],
  "engines": {
    "node": ">= 16"
  },
  "scripts": {
    "start": "pnpm --filter=@mystorage/backend start",
    "dev:start": "pnpm -r build && pnpm start",
    "build:frontend": "pnpm --filter=@mystorage/frontend build",
    "dev:backend": "pnpm --filter=@mystorage/backend run dev",
    "dev:frontend": "pnpm --filter=@mystorage/frontend run dev"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/LWJerri/MyStorage.git"
  },
